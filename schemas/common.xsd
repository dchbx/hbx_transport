			<xs:enumeration value="urn:openhbx:terms:v1:tax_filer#joint"/>
			<xs:enumeration value="urn:openhbx:terms:v1:tax_filer#separate"/>
			<xs:enumeration value="urn:openhbx:terms:v1:tax_filer#dependent"/>
			<xs:enumeration value="urn:openhbx:terms:v1:tax_filer#non-filer"/>
